     <script>
      var map;

      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 13.012147, lng: 80.238523},
          zoom: 14
        });

        var marker = new google.maps.Marker({
          position: {lat: 13.012147, lng: 80.238523},
          map: map,
          title: 'Hello World!'
        });

      }
    </script>
